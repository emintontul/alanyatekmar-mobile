{
  "name": "tekmarmobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android warning-mode=all",
    "ios": "react-native run-ios",
    "pod": "npx pod-install ios",
    "clean:android": "cd android && ./gradlew clean --warning-mode all && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "apk-mac": "cd android && ./gradlew assembleRelease  && cd ..",
    "apk-windows": "cd android && gradlew assembleRelease  && cd ..",
    "start": "react-native start",
    "start-reset-cache": "npm run start -- --reset-cache",
    "test": "jest",
    "lint": "tsc --noEmit && eslint --fix --ext .ts,.tsx ./app/**/*.{ts,tsx}",
    "pre-commit": "yarn lint && yarn format",
    "format": "prettier --write **/*.{js,ts,tsx,json} && prettier --check **/*.{js,ts,tsx,json}",
    "reset-cache": "npm start -- --reset-cache",
    "logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "icon-list": "node setup/icon-list.js",
    "app:theme": "node setup/theme.js",
    "app:name": "npx react-native-rename tekmarmobile -b com.tekmarmobile",
    "xcode": "cd ios && xed . && cd ..",
    "dev-clean": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "app:asset": "npx react-native-asset",
    "swagger-generator": "npx @rtk-query/codegen-openapi openapi-config.js"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.5.1",
    "@gorhom/portal": "^1.0.14",
    "@hookform/resolvers": "^3.3.2",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/blur": "^4.3.2",
    "@react-native-community/datetimepicker": "^7.6.2",
    "@react-native-community/netinfo": "^11.1.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "@rtk-query/codegen-openapi": "^1.2.0",
    "axios": "^1.6.2",
    "i18next": "^23.7.7",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^13.5.0",
    "react-native": "^0.72.7",
    "react-native-blob-util": "^0.19.4",
    "react-native-calendars": "^1.1302.0",
    "react-native-collapsible": "^1.6.1",
    "react-native-crypto-js": "^1.0.0",
    "react-native-device-info": "^10.12.0",
    "react-native-dotenv": "^3.4.9",
    "react-native-drop-shadow": "^1.0.0",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-icomoon": "^0.1.1",
    "react-native-image-picker": "^7.0.3",
    "react-native-image-progress": "^1.2.0",
    "react-native-image-resizer": "^1.4.5",
    "react-native-image-viewing": "^0.2.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-onesignal": "^5.0.3",
    "react-native-pdf": "^6.7.3",
    "react-native-permissions": "^3.10.1",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "^3.6.0",
    "react-native-redash": "^18.1.1",
    "react-native-render-html": "^6.3.4",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^14.0.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-toast-message": "^2.1.7",
    "react-native-webview": "^13.6.3",
    "react-number-format": "^5.3.1",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "rn-placeholder": "^3.0.3",
    "tp-react-native-bluetooth-printer": "^1.5.0",
    "ts-transformer-keys": "^0.4.4",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@react-native/eslint-config": "^0.74.0",
    "@react-native/metro-config": "^0.73.2",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.10",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-native": "^0.72.7",
    "@types/react-native-crypto-js": "^1.0.3",
    "@types/react-redux": "^7.1.31",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-traverse": "^6.26.0",
    "babylon": "^6.18.0",
    "eslint": "^8.54.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^3.1.0",
    "react-native-obfuscator": "^1.0.0",
    "react-native-rename": "^3.2.13",
    "react-test-renderer": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "app/**/*.{ts,tsx}": [
      "yarn lint"
    ],
    "./app/**": [
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=18"
  }
}
