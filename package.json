{
  "name": "figoparamobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android warning-mode=all",
    "ios": "react-native run-ios",
    "pod": "npx pod-install ios",
    "clean:android": "cd android && ./gradlew clean --warning-mode all && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "apk-mac": "cd android && ./gradlew assembleRelease  && cd ..",
    "apk-windows": "cd android && gradlew assembleRelease  && cd ..",
    "start": "react-native start",
    "start-reset-cache": "npm run start -- --reset-cache",
    "test": "jest",
    "lint": "tsc --noEmit && eslint --fix --ext .ts,.tsx ./app/**/*.{ts,tsx}",
    "pre-commit": "yarn lint && yarn format",
    "format": "prettier --write **/*.{js,ts,tsx,json} && prettier --check **/*.{js,ts,tsx,json}",
    "reset-cache": "npm start -- --reset-cache",
    "logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "icon-list": "node setup/icon-list.js",
    "app:theme": "node setup/theme.js",
    "app:name": "npx react-native-rename figoparamobile -b com.figoparamobile",
    "xcode": "cd ios && xed . && cd ..",
    "dev-clean": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "app:asset": "npx react-native-asset",
    "swagger-generator": "npx @rtk-query/codegen-openapi openapi-config.js"
  },
  "dependencies": {
    "@baronha/react-native-multiple-image-picker": "^0.4.13",
    "@gorhom/bottom-sheet": "^4.4.5",
    "@gorhom/portal": "^1.0.14",
    "@hookform/resolvers": "^2.9.10",
    "@notifee/react-native": "^7.3.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/blur": "^4.3.0",
    "@react-native-community/datetimepicker": "^6.7.1",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-native-firebase/app": "^11.2.0",
    "@react-native-firebase/messaging": "^11.2.0",
    "@react-navigation/bottom-tabs": "^6.5.2",
    "@react-navigation/drawer": "^6.5.6",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/native-stack": "^6.9.7",
    "@react-navigation/stack": "^6.3.10",
    "@reduxjs/toolkit": "^1.9.1",
    "@rtk-query/codegen-openapi": "^1.0.0",
    "axios": "^1.2.1",
    "i18next": "^22.4.6",
    "lodash": "^4.17.21",
    "lottie-react-native": "^5.1.4",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "query-string": "^8.1.0",
    "react": "18.1.0",
    "react-hook-form": "^7.41.5",
    "react-i18next": "^12.1.1",
    "react-native": "0.70.6",
    "react-native-blob-util": "^0.17.3",
    "react-native-bluetooth-escpos-printer": "github:januslo/react-native-bluetooth-escpos-printer",
    "react-native-calendars": "^1.1292.0",
    "react-native-collapsible": "^1.6.1",
    "react-native-crypto-js": "^1.0.0",
    "react-native-device-info": "^10.3.0",
    "react-native-dotenv": "^3.4.6",
    "react-native-drop-shadow": "^0.0.6",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "2.13.2",
    "react-native-icomoon": "^0.1.1",
    "react-native-image-picker": "^4.10.3",
    "react-native-image-progress": "^1.2.0",
    "react-native-image-resizer": "^1.4.5",
    "react-native-image-viewing": "^0.2.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.1",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^14.0.1",
    "react-native-onesignal": "^4.5.0",
    "react-native-pdf": "^6.6.2",
    "react-native-permissions": "^3.6.1",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "2.14.4",
    "react-native-redash": "^18.1.0",
    "react-native-render-html": "^6.3.4",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-svg": "^13.6.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-webview": "^12.0.2",
    "react-redux": "^8.0.5",
    "tp-react-native-bluetooth-printer": "git+https://github.com/kruyvanna/tp-react-native-bluetooth-printer.git",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "rn-placeholder": "^3.0.3",
    "ts-transformer-keys": "^0.4.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.70.6",
    "@types/react-native-crypto-js": "^1.0.0",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-traverse": "^6.26.0",
    "babylon": "^6.18.0",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^13.1.1",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.4.1",
    "react-native-obfuscator": "^1.0.0",
    "react-native-rename": "^2.9.0",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "app/**/*.{ts,tsx}": [
      "yarn lint"
    ],
    "./app/**": [
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=16"
  }
}